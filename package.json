    "@types/multer": "^1.3.9",
    "@types/nodemailer": "^6.2.1",
    "acl": "^0.4.11",
    "bcryptjs": "^2.4.3",
    "chrome-page-eval": "^1.2.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.10.0",
    "cors": "^2.8.5",
    "cron-scheduler": "^1.2.0",
    "crypto": "^1.0.1",
    "dotenv": "^8.1.0",
    "email-templates": "^6.0.0",
    "exceljs": "^4.1.1",
    "express": "^4.17.1",
    "fcm-node": "^1.5.2",
    "generate-password": "^1.4.2",
    "html-pdf": "^2.2.0",
    "html-to-xlsx": "^1.1.1",
    "inversify": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "method-override": "^3.0.0",
    "module-alias": "^2.2.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "mongodb": "^3.3.0-beta2",
    "multer": "^1.4.2",
    "mysql": "^2.17.1",
    "node-mailer": "^0.1.1",
    "nodemailer": "^6.3.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pug": "^2.0.4",
    "puppeteer": "^5.5.0",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "socket.io": "^2.3.0",
    "socketio-jwt": "^4.5.0",
    "speakeasy": "^2.0.0",
    "typeorm": "^0.2.18"
  },
  "devDependencies": {
    "@types/acl": "^0.4.37",
    "@types/bcryptjs": "^2.4.2",
    "@types/dotenv": "^6.1.1",
    "@types/email-templates": "^6.0.0",
    "@types/express": "^4.17.0",
    "@types/node": "^12.0.8",
    "@types/passport": "^1.0.0",
    "@types/passport-jwt": "^3.0.1",
    "@types/passport-local": "^1.0.33",
    "@types/request": "^2.48.1",
    "@types/winston": "^2.4.4",
    "nodemon": "^1.19.1",
    "ts-node": "^8.3.0",
    "tslint": "^5.17.0",
    "typescript": "^3.7.2",
    "winston": "^3.2.1"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "scripts": {
    "start": "node -r ts-node/register ./src/main.ts",
    "start:watch": "nodemon",
    "log:watch": "docker attach reconed_container",
    "restart": "docker-compose restart reconed",
    "restart:watch": "yarn restart && yarn log:watch",
    "add-package": "./scripts/add_package.sh",
    "up": "docker-compose up -d && yarn log:watch",
    "build": "rm -rf dist/* && tsc -p tsconfig.json",
    "debug": "nodemon --inspect",
    "lint": "tslint 'src/**/*{.ts,.tsx}'",
    "tslint-fix": "tslint --fix -c tslint.json -p tsconfig.json",
    "generate:invoices": "node -r ts-node/register ./src/commands/invoices-create.ts"
  },
  "_moduleAliases": {
    "@controllers": "src/controllers",
    "@connectors": "src/connectors",
    "@constants": "src/constants",
    "@repositories": "src/repositories",
    "@entities": "src/entities",
    "@commons": "src/commons",
    "@models": "src/models",
    "@mails": "src/mails",
    "@loggers": "src/loggers",
    "@validators": "src/validators",
    "@infrastructures": "src/infrastructures",
    "@utils": "src/utils",
    "@policies": "src/policies",
    "@services": "src/services",
    "@paginate": "src/paginate"
  }
}
